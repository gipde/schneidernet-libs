{
  "name": "schneidernet",
  "version": "1.0.0",
  "scripts": {
    "clean": "pnpm -r clean",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "format": "prettier  --loglevel warn --write \"./**/*.{ts,md,js,yaml,json}\" ",
    "formatCheck": "prettier  --check \"./**/*.{ts,tsx,md,js,jsx,yaml,json}\" ",
    "lint": "eslint ./ --fix",
    "check": "pnpm clean && pnpm formatCheck && pnpm lint && pnpm test"
  },
  "license": "ISC",
  "private": true,
  "devDependencies": {
    "@schneidernet/eslint-config": "0.0.9",
    "@schneidernet/prettier-config": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "eslint": "^8.7.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "prettier": "@schneidernet/prettier-config"
}
